<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup Label="Configuration">
    <RootNamespace>LinqToTwitter</RootNamespace>
    <SignAssembly>True</SignAssembly>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <Description>A LINQ Provider for the Twitter API</Description>
    <Copyright>copyright</Copyright>
    <AssemblyTitle>LinqToTwitter.net</AssemblyTitle>
    <AssemblyName>LinqToTwitter.Core</AssemblyName>
    <VersionPrefix>4.0.0</VersionPrefix>
    <AssemblyOriginatorKeyFile>..\LinqToTwitter.net\LinqToTwitter.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
    <EmbeddedResource Include="**\*.resx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="NETStandard.Library" Version="1.6" />
    <PackageReference Include="System.Linq.Queryable" Version="4.3.0" />
    <PackageReference Include="System.Reactive" Version="3.1.1" />
    <PackageReference Include="System.Xml.XmlSerializer" Version="4.3.0" />
  </ItemGroup>
  <Import Project="..\LinqToTwitter.Shared\LinqToTwitter.Shared.projitems" Label="Shared" />
  <Import Project="..\LinqToTwitter.Shared.net\LinqToTwitter.Shared.net.projitems" Label="Shared" />
</Project>